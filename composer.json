{
  "name": "pepijnolivier/laravel-poloniex",
  "description": "Poloniex API wrapper for Laravel",
  "tags": ["poloniex", "api", "laravel"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pepijn Olivier",
      "email": "olivier.pepijn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-curl": "*",
    "laravel/framework": ">=5.3"
  },
  "autoload": {
    "psr-4": {
      "Pepijnolivier\\Poloniex\\": "src/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Pepijnolivier\\Poloniex\\PoloniexServiceProvider"
        ],
        "aliases": {
            "Poloniex": "Pepijnolivier\\Poloniex\\Poloniex"
        }
    }
  },
  "minimum-stability": "dev"
}
